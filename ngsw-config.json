{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap-grid.min.css",
          "https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css",
          "https://unicons.iconscout.com/release/v3.0.6/css/line.css"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "data-api",
      "urls": [
        "https://api.github.com/users/leifermendez/repos",
        "https://raw.githubusercontent.com/wiki/leifermendez",
        "https://api.github.com/repos/leifermendez/portfolio-web",
        "https://youtube.googleapis.com/youtube/v3/playlists?key=AIzaSyC14wybawh7qeY2qyPO5LDpW6NothdvUAg"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 5,
        "maxAge": "50m",
        "timeout": "5s"
      }
    }
  ]
}
